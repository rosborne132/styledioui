{
    "name": "styledoui",
    "version": "0.1.01",
    "author": "Robert Osborne",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build-storybook-docs": "build-storybook -s public --docs",
        "build-storybook": "build-storybook -c .storybook -o build",
        "build:package": "cross-env BABEL_ENV=production babel src/lib -d dist",
        "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __snapshots__,test.js,stories.js",
        "icons:generate": "npx @svgr/cli src/assets/icons/svg --out-dir src/lib/Elements/Icon",
        "package:publish": "npm run build && npm publish --access public",
        "storybook": "start-storybook -p 9009 -s public",
        "test": "react-scripts test"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/user-event": "^7.1.2",
        "polished": "^3.5.1",
        "prop-types": "^15.7.2",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@storybook/addon-a11y": "^5.3.17",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addon-docs": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-links": "^5.3.17",
        "@storybook/addon-storyshots": "^5.3.18",
        "@storybook/addon-storysource": "^5.3.18",
        "@storybook/addons": "^5.3.17",
        "@storybook/preset-create-react-app": "^2.1.1",
        "@storybook/react": "^5.3.17",
        "@svgr/cli": "^5.3.0",
        "@testing-library/react": "^9.5.0",
        "auto": "^9.24.0",
        "eslint": "^6.2.2",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.1.0",
        "eslint-config-react-app": "^5.0.1",
        "eslint-loader": "^3.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "husky": "^3.1.0",
        "prettier": "^1.5",
        "pretty-quick": "^2.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1",
        "react-test-renderer": "^16.13.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rosborne132/styledioui"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
