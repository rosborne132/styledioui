{
    "name": "styledoui",
    "version": "0.1.1",
    "author": "Robert Osborne",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build-storybook": "build-storybook -c .storybook -o build",
        "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,test.js,stories.tsx",
        "deploy:package": "npm run build && npm publish --access public",
        "eject": "react-scripts eject",
        "icons:generate": "npx @svgr/cli src/assets/icons/svg --out-dir src/lib/Elements/Icon",
        "storybook:publish": "npm run build-storybook && now",
        "storybook": "start-storybook -p 9009 -s public",
        "test": "react-scripts test"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react-dom": "^16.9.0",
        "@types/react": "^16.9.0",
        "typescript": "~3.7.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addon-links": "^5.3.17",
        "@storybook/addons": "^5.3.17",
        "@storybook/preset-create-react-app": "^2.1.1",
        "@storybook/react": "^5.3.17",
        "@svgr/cli": "^5.3.0",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.1.0",
        "eslint-config-react-app": "^5.0.1",
        "eslint-loader": "^3.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "eslint-plugin-react": "^7.14.3",
        "eslint": "^6.2.2",
        "husky": "^3.1.0",
        "prettier": "^1.5",
        "pretty-quick": "^2.0.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1",
        "react": "^16.13.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rosborne132/styledioui"
    }
}
